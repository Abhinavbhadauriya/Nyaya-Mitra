<link rel="stylesheet" href="/css/streamindex.css">

<div class="streams-section">
<h2>All Streams</h2>
<ul>
  <% streams.forEach(stream => { %>
    <li><%= stream.courtName %></li>
     <a href="<%= stream.youtubeLink %>"><%= stream.youtubeLink %></a>
    <li><%= stream.startedAt %></li>
    <br>
  <% }) %>
</ul>
</div>
